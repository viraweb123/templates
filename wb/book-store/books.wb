tif (!params.has('_px_p')) {\n\t\t\tparams.set('_px_p', 1);\n\t\t}\n\t\tif (!params.has('_px_ps')) {\n\t\t\tparams.set('_px_ps', 32);\n\t\t}\n\t\tvar keys = params.keys();\n\t\tvar paramsJson = {};\n\t\tfor (const key of keys) {\n\t\t\tparamsJson[key] = params.get(key);\n\t\t}\n\t\tmyScope.params = paramsJson;\n\t}\n\treturn myScope.params;\n}\n\n// Edit data\nfunction editData(item) {\n\t// Shorten description\n\t// item.description = shortenStr(item.description, 300);\n\t// Add metas\n\titem.metamap = {};\n\titem.metas.forEach(function(meta) {\n\t\tvar k = meta.key.replace(/\\./g, '_');\n\t\titem.metamap[k] = meta.value;\n\t});\n\t// Data for microdata\n\titem.creation_dtime_iso = new Date(item.creation_dtime).toISOString();\n\titem.modif_dtime_iso = new Date(item.modif_dtime).toISOString();\n\treturn item;\n}\n\n// Shorten string\nfunction shortenStr(str, len) {\n\tvar shortStr = str;\n\tif (str.length > len) {\n\t\tshortStr = str.substr(0, len);\n\t\tshortStr = shortStr + '...';\n\t}\n\treturn shortStr;\n}\n\nfunction initPaginator(pag) {\n\tvar params = $location.search();\n\tparams = new URLSearchParams(params);\n\tvar ulPag = $('#ul-paginator');\n\tfor (var i = 1; i <= pag.page_number; i++) {\n\t\tparams.set('_px_p', i);\n\t\tvar link = $location.absUrl();\n\t\tlink = link.indexOf('?') >= 0 ? link.substring(0, link.indexOf('?')) : link;\n\t\tlink = link + '?' + params.toString()\n\t\tvar li = $('<li class=\"page-item\"><a class=\"pagination-btn page-link\" data-index=\"'+i+'\" href=\"' + link + '\">' + i + '</a></li>');\n\t\tif(i == pag.current_page){\n\t\t\tli.addClass('active');\n\t\t}\n\t\tulPag.append(li);\n\t}\n\t$('.pagination-btn').click(paginatorBtnClicked);\n}\n\nfunction createCollection(itemsList) {\n\tvar collectionWidget = $widget.getChildById('books');\n\t// Clear collections\n\tcollectionWidget.removeChildren();\n\tif (myScope.collectionBuilder == null) { // First call, create and init collection builder\n\t\t// Get post item template\n\t\tvar template = $widget.getChildById('section-template')\n\t\t\t.getChildById('article-tmpl').getModel();\n\t\tvar template = angular.copy(template);\n\n\t\t// Create collection builder\n\t\tmyScope.collectionBuilder = new CollectionBuilder();\n\t\tmyScope.collectionBuilder.setTemplate(template)\n\t\t\t.setWidget(collectionWidget);\n\t}\n\tmyScope.collectionBuilder.setItems(itemsList)\n\t\t.build();\n}\n\nfunction paginatorBtnClicked(ev) {\n\tev.preventDefault();\n\tev.stopImmediatePropagation();\n\tev.stopPropagation();\n\n\t$('.page-item').removeClass('active');\n\t$(ev.target).parent().addClass('active');\n\tvar index = $(ev.target).attr('data-index');\n\tif ($widget.state == 'ready') {\n\t\tvar pag = myScope.paginator;\n\t\tif (pag.current_page != index) {\n\t\t\tvar params = getParams();\n\t\t\tparams._px_p = index;\n\t\t\treturn loadItems(params);\n\t\t}\n\t}\n}\n"},"version":"wb4","children":[{"type":"div","children":[{"style":{"position":"relative"},"type":"h1","on":{},"id":"h1-title","version":"wb4","class":"mx-auto","html":"<b>All EBooks</b>"}],"on":{"init":"var brand = $window.getMeta('theme:brand_title');\n// Set the URL\nvar url = null;\nvar catId = $routeParams['catId'];\nvar tagId = $routeParams['tagId'];\nif (catId) {\n\turl = '/api/v2/sdp/categories/' + catId;\n} else if (tagId) {\n\turl = '/api/v2/sdp/tags/' + tagId;\n}\n\nvar h1Title = $widget.getChildById('h1-title');\nif (url) {\n\t$http.get(url)\n\t\t.then(function(res) {\n\t\t\tvar data = res.data;\n\t\t\th1Title.setProperty('html', '<b>' + data.name + '</b>');\n\t\t\tsetSeoInfo(data);\n\t\t});\n} else {\n\tdata = {\n\t\tname: 'All EBooks',\n\t\tdescription: 'You can find many many ebooks on the Knowsmore`s` free ebooks'\n\t};\n\th1Title.setProperty('html', '<b>' + data.name + '</b>');\n\tsetSeoInfo(data);\n}\n\nfunction setSeoInfo(data) {\n\t$document.attr('title', data.name + ' | ' + brand);\n\t$window.setMeta('description', data.description);\n\tvar params = $window.location.search;\n\tvar canLi