n\t.getChildById('ul-thumbnails');\n// thumbnail items template                    \nvar liThumbTmpl = divRow.getChildById('col-thumbnails')\n\t.getChildById('product-thumbnails')\n\t.getChildById('templates')\n\t.getChildById('li-thumb-template');\n// main image div                    \nvar imgDiv = divRow.getChildById('col-image')\n\t.getChildById('product-image')\n\t.getChildById('image-background');\n\nvar metaImg = divRow.getChildById('col-image')\n\t.getChildById('product-image')\n\t.getChildById('meta-img');\n\n// Product details widgets                    \nvar divDetail = rowProduct.getChildById('div-product-details')\n\t.getChildById('details');\n\nvar hTitle = divDetail.getChildById('div-title')\n\t.getChildById('h2-title');\n\nvar pDesc = divDetail.getChildById('div-title')\n\t.getChildById('p-description');\n\nvar spanPrice = divDetail.getChildById('div-price')\n\t.getChildById('span-price');\nvar spanTotalPrice = divDetail.getChildById('div-total-price')\n\t.getChildById('span-total-price');\n\nvar metaPrice = divDetail.getChildById('div-total-price')\n\t.getChildById('meta-price');\n\nvar metaCurrency = divDetail.getChildById('div-total-price')\n\t.getChildById('meta-currency');\n\nvar metaUrl = divDetail.getChildById('div-total-price')\n\t.getChildById('meta-url');\n\nvar metaAvail = divDetail.getChildById('div-total-price')\n\t.getChildById('meta-availablility');\n\nvar divDelivery = divDetail.getChildById('div-delivery');\n\nvar tableBody = divDetail.getChildById('table-info')\n\t.getChildById('table-body');\n\nvar formattCurrency = function(val) {\n\treturn Number(val).toLocaleString();\n};\n\nvar toFarsiNumber = function(n) {\n\tconst farsiDigits = ['۰', '۱', '۲', '۳', '۴', '۵', '۶', '۷', '۸', '۹'];\n\treturn n\n\t\t.toString()\n\t\t.replace(/\\d/g, x => farsiDigits[x]);\n};\n\n// Edit data\nfunction createData(item) {\n\tvar data = angular.copy(item);\n\tdata.off = data.off ? data.off : 0;\n\tdata.price = data.price ? data.price : 0;\n\tdata.total_price = data.price - data.off;\n\tdata.price_numerical = data.total_price;\n\t// Format prices\n\tdata.price = toFarsiNumber(formattCurrency(data.price)) + ' ' + $widget.options.currency;\n\tdata.total_price = toFarsiNumber(formattCurrency(data.total_price)) + ' ' + $widget.options.currency;\n\tdata.off_txt = toFarsiNumber(formattCurrency(data.off));\n\n\tvar catList = [];\n\tdata.categories.forEach(function(cat) {\n\t\tcatList.push(cat.name);\n\t});\n\tdata.categoriesStr = catList.join(' ');\n\n\treturn data;\n}\n\nfunction setProductInfo(data) {\n\thTitle.setProperty('html', data.title);\n\tpDesc.setProperty('html', data.description ? data.description : ' ');\n\tspanPrice.setProperty('html', data.price);\n\tspanTotalPrice.setProperty('html', data.total_price);\n\timgDiv.setProperty('style.backgroundImage', 'url(\"/imgx' + data.avatar + '\")');\n\tif (data.off == 0) {\n\t\tspanPrice.setProperty('style.display', 'none');\n\t}\n\tif (data.delivery) {\n\t\tdivDelivery.getChildById('span-delivery')\n\t\t\t.setProperty('html', data.delivery);\n\t} else {\n\t\tdivDelivery.setProperty('style.display', 'none!important');\n\t}\n\t// Additional Information\n\tvar infoStr = '';\n\tif (data.additional_info) {\n\t\tdata.additional_info.forEach(function(info) {\n\t\t\tinfoStr += '<tr id=\"tr-' + info.key + '\"><td class=\"first-col\">' + info.key + '</td><td class=\"second-col\">' + info.value + '</td></tr>';\n\t\t});\n\t\ttableBody.setProperty('html', infoStr);\n\t};\n\n\t// Set microdata about image, price and currency\n\tmetaImg.setProperty('content', data.avatar);\n\tmetaPrice.setProperty('content', data.price_numerical);\n\tmetaCurrency.setProperty('content', $widget.options.currency_code);\n\tmetaUrl.setProperty('content', '/wb/shop/product?id=' + data.id);\n\tmetaAvail.setProperty('content', 'http://schema.org/OnlineOnly');\n}\n\n\nfunction setSeoInfo(data) {\n\tvar url = '/wb/shop/product?id=' + data.id;\n\t$window.setLink('canonical', {\n\t\t'rel': 'canonical',\n\t\t'href': url\n\t});\n\t$window.setLink('favicon', {\n\t\t'rel': 'icon',\n\t\t'href': '/api/v2/cms/contents/logo/content'\n\t});\n\t$document.attr('title', data.title + ' | ' + $widget.options.brand);\n\t$window.setMeta('description', data.description);\n\t$window.setMeta('link.cover', data.avatar);\n\t$window.setMeta('og:image', data.avatar);\n}\n\nfunction createThumbnailsCollection(thumbList) {\n\tvar template = angular.copy(liThumbTmpl.getModel());\n\n\t// Build collection for thumbnails\n\tvar collectionBuilder = new CollectionBuilder();\n\treturn collectionBuilder.setTemplate(template)\n\t\t.setItems(thumbList)\n\t\t.setWidget(ulThumbnails)\n\t\t.build();\n}\n\nfunction createBreadcrumbsCollection(data) {\n\t// Prepare items in the path\n\tvar pathList = [];\n\tif (data.categories && data.categories.length > 0) {\n\t\tvar cat = data.categories[0];\n\t\tdo {\n\t\t\tcat.url = '/wb/shop/category?id=' + cat.id;\n\t\t\tpathList.push(cat);\n\t\t\tcat = cat.parent;\n\t\t} while (cat != null);\n\t}\n\tpathList.push({\n\t\tname: $widget.options.shop_title,\n\t\turl: $widget.options.shop_link,\n\t\tfirst: true\n\t});\n\tpathList.unshift({\n\t\tname: data.title,\n\t\turl: '/wb/shop/product?id=' + data.id,\n\t\tlast: true\n\t});\n\t// Build collection for breadcrumb (path)\n\tvar template = angular.copy(breadcrumbTmpl.getModel());\n\tvar collectionBuilder = new CollectionBuilder();\n\treturn collectionBuilder.setTemplate(template)\n\t\t.setItems(pathList)\n\t\t.setWidget(ulBreadcrumbs)\n\t\t.build();\n}\n\nfunction init(pageIndex) {\n\t// Clear collections\n\tulThumbnails.removeChildren();\n\tulBreadcrumbs.removeChildren();\n\t// Do request\n\treturn $http({\n\t\tmethod: 'GET',\n\t\turl: $widget.options.url,\n\t\tparams: {\n\t\t\t// Categories will be fetch up to 3 level\n\t\t\tgraphql: '{id,title,description,price,off,avatar,categories{id,name,parent{id,name,parent{id,name}}},metafields{key,value,namespace}}'\n\t\t}\n\t}).then(function(result) {\n\t\tvar data = result.data;\n\t\tvar additionalInfo = [];\n\t\tvar thumbnails = [{\n\t\t\tsrc: '/imgx' + data.avatar,\n\t\t\tsrc_thumb: '/imgx' + data.avatar + '?w=150&h=150&f=cover&p=center'\n\t\t}];\n\t\t// create list of thumbnails and additional infos\n\t\tdata.metafields.forEach(function(item) {\n\t\t\tif (item.key == 'delivery') {\n\t\t\t\tdata.delivery = item.value;\n\t\t\t} else if (item.key.startsWith('image')) {\n\t\t\t\tvar temp = {\n\t\t\t\t\tsrc: '/imgx' + item.value,\n\t\t\t\t\tsrc_thumb: '/imgx' + item.value + '?w=150&h=150&f=cover&p=center'\n\t\t\t\t};\n\t\t\t\tthumbnails.push(temp);\n\t\t\t} else if (item.namespace == 'additional_info') {\n\t\t\t\tadditionalInfo.push(item);\n\t\t\t}\n\t\t});\n\t\t// Edit data of product\n\t\tdata = createData(data);\n\t\tdata.thumbnails = thumbnails;\n\t\tdata.additional_info = additionalInfo;\n\t\t// Set product data\n\t\tsetProductInfo(data);\n\t\t// Set SEO infos\n\t\tsetSeoInfo(data);\n\t\t// Create thumbnails\n\t\tcreateThumbnailsCollection(thumbnails);\n\t\t// Create sequence of categories\n\t\tcreateBreadcrumbsCollection(data);\n\t});\n\n}\n\ninit();\n\n"},"version":"wb4","id":"main-content"},{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":"/* CSS Document */\n\n/******************************\n\n[Table of Contents]\n\n1. 1600px\n2. 1440px\n3. 1280px\n4. 1199px\n5. 1024px\n6. 991px\n7. 959px\n8. 880px\n9. 768px\n10. 767px\n11. 539px\n12. 479px\n13. 400px\n\n******************************/\n\n\n@media only screen and (max-width: 1600px)\n{\n\t\n}\n\n/************\n2. 1440px\n************/\n\n@media only screen and (max-width: 1540px)\n{\n\t\n}\n\n/************\n3. 1380px\n************/\n\n@media only screen and (max-width: 1380px)\n{\n\t\n}\n\n/************\n3. 1280px\n************/\n\n@media only screen and (max-width: 1280px)\n{\n\t\n}\n\n/************\n4. 1199px\n************/\n\n@media only screen and (max-width: 1199px)\n{\n\t.single_product_thumbnails ul li\n\t{\n\t\theight: 110px;\n\t}\n\t.single_product