rl: $widget.options.url,\n        params: {\n            _px_sk: 'name',\n            _px_so: 'a',\n            _px_fk: 'parent_id',\n            _px_fv: catId\n        }    \n    }).then(function(result){\n        result.data.items.forEach(function(item){\n            item.active = (item.id == catId);\n        });\n        createCategoriesCollection(result.data.items);\n    });\n    \n}\n\ninit();\n\n"},"version":"wb4","id":"div-sidebar"}],"on":{},"version":"wb4","id":"div-row"}],"on":{},"version":"wb4","id":"product_section_container"},{"style":{},"type":"import","name":"Import","on":{},"version":"wb4","children":[{"style":{},"type":"div","class":"benefit","children":[{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":"\n/*********************************\n8. Benefit\n*********************************/\n\n.benefit\n{\n\tmargin-top: 74px;\n}\n.benefit_row\n{\n\tpadding-left: 15px;\n\tpadding-right: 15px;\n}\n.benefit_col\n{\n\tpadding-left: 0px;\n\tpadding-right: 0px;\n}\n.benefit_item\n{\n\theight: 100px;\n\tbackground: #f3f3f3;\n\tborder-right: solid 1px #FFFFFF;\n\tpadding-left: 25px;\n}\n.benefit_col:last-child .benefit_item\n{\n\tborder-right: none;\n}\n.benefit_icon i\n{\n\tfont-size: 30px;\n\tcolor: #fe4c50;\n}\n.benefit_content\n{\n\tpadding-left: 22px;\n}\n.benefit_content h6\n{\n\ttext-transform: uppercase;\n\tline-height: 18px;\n\tfont-weight: 500;\n\tmargin-bottom: 0px;\n}\n.benefit_content p\n{\n\tfont-size: 12px;\n\tline-height: 18px;\n\tmargin-bottom: 0px;\n\tcolor: #51545f;\n}\n\n\n/*********************************\nBenefit Responsive\n*********************************/\n\n@media only screen and (max-width: 991px)\n{\n    .benefit_col\n\t{\n\t\tmargin-bottom: 30px;\n\t}\n\t.benefit_col:last-child\n\t{\n\t\tmargin-bottom: 0px;\n\t}\n}\n\n@media only screen and (max-width: 767px)\n{\n    .benefit_col\n\t{\n\t\tmargin-bottom: 15px;\n\t}\n}\n","id":"style-benefit"},{"style":{},"type":"div","class":"container","children":[{"style":