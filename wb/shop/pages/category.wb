ef":"/","on":{},"version":"wb4","html":"صفحه اصلی"}],"on":{},"version":"wb4"},{"style":{},"type":"li","class":"menu_item","children":[{"style":{},"type":"a","href":"/wb/shop","html":"فروشگاه","on":{},"version":"wb4"}],"on":{},"version":"wb4"},{"style":{},"type":"li","class":"menu_item","children":[{"style":{},"type":"a","href":"/wb/blog","html":"بلاگ","on":{},"version":"wb4"}],"on":{},"version":"wb4"},{"style":{},"type":"li","class":"menu_item","children":[{"style":{},"type":"a","href":"/wb/contact-us","html":"تماس با ما","on":{},"version":"wb4"}],"on":{},"version":"wb4"},{"style":{},"type":"li","class":"menu_item","children":[{"style":{},"type":"a","href":"/wb/about-us","html":"درباره ما","on":{},"version":"wb4"}],"on":{},"version":"wb4"}],"on":{},"version":"wb4"}],"on":{},"version":"wb4"}],"on":{},"version":"wb4","id":"hamburger_menu"}],"id":"main-header"}],"url":"/api/v2/cms/contents/shop-common-widgets/content#main-header","id":"import-header"},{"style":{},"type":"div","class":"container product_section_container","children":[{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":"\n/*********************************\n6. product section\n*********************************/\n\n.product_section_container\n{\n\tmargin-top: 150px;\n}\n\n.product_section\n{\n\n}\n\n/*********************************\nproduct section responsive\n*********************************/\n\n@media only screen and (max-width: 991px)\n{\n    .product_section_container\n\t{\n\t\tmargin-top: 100px;\n\t}\n}","id":"style-product-section"},{"style":{},"type":"div","class":"breadcrumbs d-flex flex-row align-items-center","children":[{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":"\n/*********************************\n5. Breadcrumbs\n*********************************/\n\n.breadcrumbs\n{\n\twidth: 100%;\n\theight: 60px;\n\tborder-bottom: solid 1px #ebebeb;\n\tmargin-bottom: 58px;\n\tdirection: rtl;\n\ttext-align: right;\n}\n.breadcrumbs ul li\n{\n\tdisplay: inline-block;\n\tmargin-left: 17px;\n}\n.breadcrumbs ul li.active a\n{\n\tcolor: #b9b4c7;\n}\n.breadcrumbs ul li i\n{\n\tmargin-left: 17px;\n}\n.breadcrumbs ul li a\n{\n\tcolor: #51545f;\n}","id":"style-breadcrumbs"},{"style":{},"type":"ul","children":[],"on":{"init":"$widget.on('stateChanged', function(){\n    if($widget.state == 'edit'){\n        $widget.removeChildren();\n    }\n});"},"version":"wb4","id":"ul-breadcrumbs"},{"style":{},"type":"ul","children":[{"style":{},"type":"li","class":"{{#last}}active{{/last}}","children":[{"style":{},"type":"a","html":"{{^first}}<i class=\"fa fa-angle-left\" aria-hidden=\"true\"></i>{{/first}}{{name}}","on":{},"version":"wb4","href":"{{{url}}}","class":"breadcrumb-item"}],"on":{},"version":"wb4","id":"li-path-tmpl"}],"on":{"init":"$widget.on('stateChanged', function(){\n    if($widget.state == 'ready'){\n        $widget.setProperty('style.display', 'none');\n    }else{\n        $widget.setProperty('style.display');\n    }\n});"},"version":"wb4","id":"templates"}],"on":{"init":"var catId = $routeParams.id;\nvar url = '/api/v2/shop/categories/' + catId;\n\n$widget.options = {\n    url: url,\n    shop_title: $window.getMeta('theme:breadcrumb.shop_link_title'),\n\tshop_link: $window.getMeta('theme:breadcrumb.shop_link')\n};\n\n// Breadcrumb section\nvar breadcrumbDiv = $widget;\n\nvar breadcrumbTmpl = breadcrumbDiv.getChildById('templates')\n                    .getChildById('li-path-tmpl');\n                    \nvar ulBreadcrumbs = breadcrumbDiv.getChildById('ul-breadcrumbs');\n\n\nfunction createBreadcrumbsCollection(data){\n    // Prepare items in the path\n    var pathList = [];\n    if(data.parent && data.parent.id > 0){\n        var cat = data.parent;\n        do{\n            cat.url = '/wb/shop/category?id=' + cat.id;\n            pathList.unshift(cat);\n            cat = cat.parent;\n        }while(cat != null);\n    }\n    pathList.push({\n\t\tname: $widget.options.shop_title,\n\t\turl: $widget.options.shop_link,\n\t\tfirst: true\n\t});\n\tpathList.unshift({\n\t\tname: data.name,\n        url: '/wb/shop/category?id=' + data.id,\n        last: true\n\t});\n    // Build collection for breadcrumb (path)\n    var template = angular.copy(breadcrumbTmpl.getModel());\n    var collectionBuilder = new CollectionBuilder();\n    return collectionBuilder.setTemplate(template)\n        .setItems(pathList)\n        .setWidget(ulBreadcrumbs)\n        .build();\n}\n\nfunction init(){\n    // Clear collections\n    ulBreadcrumbs.removeChildren();\n    if(!catId){\n        return;\n    }\n    // Do request\n    return $http({\n        method: 'GET',\n        url: $widget.options.url,\n        params: {\n            // Categories will be fetch up to 3 level\n            graphql: '{id,name,description,parent{id,name,parent{id,name}}}'\n        }    \n    }).then(function(result){\n        var data = result.data;\n        // Create sequence of categories\n        createBreadcrumbsCollection(data);\n    });\n    \n}\n\ninit();\n\n"},"version":"wb4","id":"div-breadcrumbs"},{"style":{},"type":"div","class":"row","children":[{"style":{},"type":"div","class":"container main_content","children":[{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":"\n/*********************************\n4. Products\n*********************************/\n\n.main_content\n{\n\tfloat: right;\n\twidth: calc(100% - 200px);\n\tpadding-right: 34px;\n}\n\n.product-grid\n{\n\twidth: 100%;\n\tmargin-top: 57px;\n\tjustify-content: center;\n}\n\n.section_title\n{\n\tdisplay: inline-block;\n\ttext-align: center;\n}\n.section_title h2\n{\n\tdisplay: inline-block;\n}\n.section_title::after\n{\n\tdisplay: block;\n\tposition: absolute;\n\ttop: calc(100% + 13px);\n\tleft: 50%;\n\t-webkit-transform: translateX(-50%);\n\t-moz-transform: translateX(-50%);\n\t-ms-transform: translateX(-50%);\n\t-o-transform: translateX(-50%);\n\ttransform: translateX(-50%);\n\twidth: 60px;\n\theight: 5px;\n\tbackground: #fe4c50;\n\tcontent: '';\n}\n\n/*********************************\n4.1 Product Sorting\n*********************************/\n\n.product_sorting_container_bottom\n{\n\tmargin-top: 24px;\n}\n.product_sorting_container_bottom .product_sorting\n{\n\tfloat: right;\n}\n.product_sorting\n{\n\tdisplay: inline-block;\n}\n.product_sorting > li\n{\n\tdisplay: inline-block;\n\tposition: relative;\n\twidth: 185px;\n\theight: 40px;\n\tline-height: 40px;\n\tcursor: pointer;\n\tborder: solid 1px #ecedee;\n\tpadding-left: 20px;\n\tpadding-right: 7px;\n\tmargin-right: 16px;\n}\n.product_sorting_container_bottom .product_sorting > li\n{\n\twidth: 144px;\n\tmargin-right: 0px;\n\tpadding-right: 12px;\n}\n.product_sorting_container_bottom .product_sorting li span:nth-child(2)\n{\n\tmargin-left: 24px;\n}\n.product_sorting li i\n{\n\tfloat: right;\n\tline-height: 40px;\n\tcolor: #51545f;\n}\n.product_sorting li ul\n{\n\tdisplay: block;\n\tposition: absolute;\n\tright: 0;\n\ttop: 120%;\n\tmargin: 0;\n\twidth: 100%;\n\tbackground: #FFFFFF;\n\tvisibility: hidden;\n\topacity: 0;\n\tz-index: 1;\n\tbox-shadow: 0 15px 25px rgba(63, 78, 100, 0.15);\n\t-webkit-transition: opacity 0.3s ease;\n\t-moz-transition: opacity 0.3s ease;\n\t-ms-transition: opacity 0.3s ease;\n\t-o-transition: opacity 0.3s ease;\n\ttransition: all 0.3s ease;\n}\n.product_sorting > li:hover ul\n{\n\tvisibility: visible;\n\topacity: 1;\n\ttop: calc(100% + 1px);\n}\n.product_sorting li ul li\n{\n\tdisplay: block;\n\ttext-align: center;\n\tpadding-left: 10px;\n\tpadding-right: 10px;\n}\n.product_sorting li ul li span\n{\n\tdisplay: block;\n\theight: 40px;\n\tline-height: 40px;\n\tborder-bottom: solid 1px #dddddd;\n\tcolor: #51545f;\n\t-webkit-transition: opacity 0.3s ease;\n\t-moz-transition: opacity 0.3s ease;\n\t-ms-transition: opacity 0.3s ease;\n\t-o-transition: opacity 0.3s ease;\n\ttransition: all 0.3s ease;\n}\n.product_sorting li ul li span:hover\n{\n\tcolor: #b5aec4;\n}\n.product_sorting li ul li:last-child span\n{\n\tborder-bottom: none;\n}\n.product_sorting li span:nth-child(2)\n{\n\tmargin-left: 71px;\n}\n.pages\n{\n\tdisplay: inline-block;\n\tfloat: right;\n}\n.product_sorting_container_bottom .pages\n{\n\tfloat: left;\n}\n.page_current\n{\n\tdisplay: inline-block;\n\tposition: relative;\n\twidth: 40px;\n\theight: 40px;\n\tline-height: 40px;\n\tcursor: pointer;\n\ttext-align: center;\n\tborder: solid 1px #ecedee;\n\tmargin-right: 16px;\n}\n.product_sorting_container_bottom .pages .page_current\n{\n\tbackground: #fe4c50;\n\tborder: none;\n}\n.page_current span\n{\n\tcolor: #51545f;\n}\n.product_sorting_container_bottom .pages .page_current span\n{\n\tcolor: #FFFFFF;\n}\n.page_selection\n{\n\tdisplay: block;\n\tposition: absolute;\n\tright: 0;\n\ttop: 120%;\n\tmargin: 0;\n\twidth: 100%;\n\tbackground: #FFFFFF;\n\tvisibility: hidden;\n\topacity: 0;\n\tz-index: 1;\n\tbox-shadow: 0 15px 25px rgba(63, 78, 100, 0.15);\n\t-webkit-transition: opacity 0.3s ease;\n\t-moz-transition: opacity 0.3s ease;\n\t-ms-transition: opacity 0.3s ease;\n\t-o-transition: opacity 0.3s ease;\n\ttransition: all 0.3s ease;\n}\n.page_selection li\n{\n\tdisplay: block;\n\ttext-align: center;\n\tpadding-left: 10px;\n\tpadding-right: 10px;\n}\n.page_selection li a\n{\n\tdisplay: block;\n\theight: 40px;\n\tline-height: 40px;\n\tborder-bottom: solid 1px #dddddd;\n\tcolor: #51545f;\n\t-webkit-transition: opacity 0.3s ease;\n\t-moz-transition: opacity 0.3s ease;\n\t-ms-transition: opacity 0.3s ease;\n\t-o-transition: opacity 0.3s ease;\n\ttransition: all 0.3s ease;\n}\n.page_selection li a:hover\n{\n\tcolor: #b5aec4;\n}\n.page_current:hover .page_selection\n{\n\tvisibility: visible;\n\topacity: 1;\n\ttop: calc(100% + 1px);\n}\n.page_total\n{\n\tdisplay: inline-block;\n\tline-height: 40px;\n\tmargin-right: 31px;\n}\n.page_total span\n{\n\tmargin-right: 14px;\n}\n.page_next\n{\n\tdisplay: inline-block;\n\tline-height: 40px;\n\tcursor: pointer;\n}\n.page_next:hover i\n{\n\tcolor: #b5aec4;\n}\n.page_next i\n{\n\tfont-size: 18px;\n\tcolor: #51545f;\n}\n.showing_results\n{\n\tfont-size: 16px;\n\theight: 40px;\n\tline-height: 40px;\n\tfloat: right;\n\tmargin-right: 34px;\n}\n\n\n/*********************************\nProducts Responsive\n*********************************/\n\n@media only screen and (max-width: 991px)\n{\n\t.main_content\n\t{\n\t\tfloat: none;\n\t\twidth: 100%;\n\t\tpadding-right: 15px;\n\t}\n\t.grid-item\n\t{\n\t\twidth: 25%;\n\t}\n}\n\n@media only screen and (max-width: 767px)\n{\n\t.product_sorting\n\t{\n\t\tdisplay: block;\n\t}\n\t.product_sorting > li\n\t{\n\t\tdisplay: block;\n\t\tmargin-bottom: 15px;\n\t}\n\t.pages\n\t{\n\t\tfloat: none;\n\t}\n\t.showing_results\n\t{\n\t\tdisplay: block;\n\t\tfloat: none;\n\t\tmargin-bottom: 15px;\n\t}\n\t.product_sorting_container_bottom .product_sorting\n\t{\n\t\tfloat: none;\n\t}\n\t.product_sorting_container_bottom .pages\n\t{\n\t\tfloat: none;\n\t}\n}\n\n\n@media only screen and (max-width: 479px)\n{\n\t.section_title::after\n\t{\n\t    top: calc(100% + 8px);\n\t    height: 3px;\n\t}\n}","id":"style-products-section"},{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":"\n/*********************************\nProduct Item\n*********************************/\n\n.product-item\n{\n\twidth: 25%;\n\tmin-width: 190px;\n\theight: 380px;\n\tcursor: pointer;\n}\n.product-item::after\n{\n\tdisplay: block;\n\tposition: absolute;\n\ttop: 0;\n\tleft: -1px;\n\twidth: calc(100% + 1px);\n\theight: 100%;\n\tpointer-events: none;\n\tcontent: '';\n\tborder: solid 2px rgba(235,235,235,0);\n\tborder-radius: 3px;\n\t-webkit-transition: all 0.3s ease;\n\t-moz-transition: all 0.3s ease;\n\t-ms-transition: all 0.3s ease;\n\t-o-transition: all 0.3s ease;\n\ttransition: all 0.3s ease;\n}\n.product-item:hover::after\n{\n\tbox-shadow: 0 25px 29px rgba(63, 78, 100, 0.15);\n\tborder: solid 2px rgba(235,235,235,1);\n}\n.product\n{\n\twidth: 100%;\n\theight: 340px;\n\tborder-right: solid 1px #e9e9e9;\n}\n.product_image\n{\n\twidth: 100%;\n}\n.product_image img\n{\n\twidth: 100%;\n}\n.product_info\n{\n\ttext-align: center;\n\tpadding-left: 10px;\n\tpadding-right: 10px;\n\tdirection: rtl;\n}\n.product_name\n{\n\tmargin-top: 27px;\n}\n.product_name a\n{\n\tcolor: #1e1e27;\n\tline-height: 20px;\n}\n.product_name:hover a\n{\n\tcolor: #b5aec4;\n}\n.discount .product_info .product_price\n{\n\tfont-size: 14px;\n}\n.product_price\n{\n\tfont-size: 16px;\n\tcolor: #fe4c50;\n\tfont-weight: 600;\n}\n.product_price span.off\n{\n\tfont-size: 12px;\n\tmargin-left: 10px;\n\tcolor: #b5aec4;\n\ttext-decoration: line-through;\n}\n.product_bubble\n{\n\tposition: absolute;\n\ttop: 15px;\n\twidth: 50px;\n\theight: 22px;\n\tborder-radius: 3px;\n\ttext-transform: uppercase;\n}\n.product_bubble span\n{\n\tfont-size: 12px;\n\tfont-weight: 600;\n\tcolor: #FFFFFF;\n}\n.product_bubble_right::after\n{\n\tcontent: ' ';\n\tposition: absolute;\n\twidth: 0;\n\theight: 0;\n\tleft: 6px;\n\tright: auto;\n\ttop: auto;\n\tbottom: -6px;\n\tborder: 6px solid;\n}\n.product_bubble_left::after\n{\n\tcontent: ' ';\n\tposition: absolute;\n\twidth: 0;\n\theight: 0;\n\tleft: auto;\n\tright: 6px;\n\tbottom: -6px;\n\tborder: 6px solid;\n}\n.product_bubble_red::after\n{\n\tborder-color: transparent transparent transparent #fe4c50;\n}\n.product_bubble_left::after\n{\n\tborder-color: #51a042 #51a042 transparent transparent;\n}\n.product_bubble_red\n{\n\tbackground: #fe4c50;\n}\n.product_bubble_green\n{\n\tbackground: #51a042;\n}\n.product_bubble_left\n{\n\tleft: 15px;\n}\n.product_bubble_right\n{\n\tright: 15px;\n}\n.add_to_cart_button\n{\n\twidth: 100%;\n\tvisibility: hidden;\n\topacity: 0;\n\t-webkit-transition: all 0.3s ease;\n\t-moz-transition: all 0.3s ease;\n\t-ms-transition: all 0.3s ease;\n\t-o-transition: all 0.3s ease;\n\ttransition: all 0.3s ease;\n}\n.add_to_cart_button a\n{\n\tfont-size: 12px;\n\tfont-weight: 600;\n}\n.product-item:hover .add_to_cart_button\n{\n\tvisibility: visible;\n\topacity: 1;\n}\n.favorite\n{\n\tposition: absolute;\n\ttop: 15px;\n\tright: 15px;\n\tcolor: #b9b4c7;\n\twidth: 17px;\n\theight: 17px;\n\tvisibility: hidden;\n\topacity: 0;\n}\n.favorite_left\n{\n\tleft: 15px;\n}\n.favorite.active\n{\n\tvisibility: visible;\n\topacity: 1;\n}\n.product-item:hover .favorite\n{\n\tvisibility: visible;\n\topacity: 1;\n}\n.favorite.active::after\n{\n\tfont-family: 'FontAwesome';\n\tcontent:'\\f004';\n\tcolor: #fe4c50;\n}\n.favorite::after\n{\n\tfont-family: 'FontAwesome';\n\tcontent:'\\f08a';\n}\n.favorite:hover::after\n{\n\tcolor: #fe4c50;\n}\n\n\n/*********************************\nProduct Item Responsive\n*********************************/\n\n@media only screen and (max-width: 1199px)\n{\n\t.product-item\n\t{\n\t\theight: 360px;\n\t}\n\t.product\n\t{\n\t\theight: 320px;\n\t}\n}\n\n@media only screen and (max-width: 991px)\n{\n\t.product-item\n\t{\n\t\twidth: 33.33333333%;\n\t}\n}\n\n@media only screen and (max-width: 767px)\n{\n\t.product-item\n\t{\n\t\twidth: 33.333333333333%;\n\t\theight: 345px;\n\t}\n\t.product\n\t{\n\t\theight: 305px;\n\t}\n}\n\n@media only screen and (max-width: 575px)\n{\n\t.product-item\n\t{\n\t\twidth: 50%;\n\t\theight: 420px;\n\t}\n\t.product\n\t{\n\t\theight: 380px;\n\t}\n}\n\n\n@media only screen and (max-width: 479px)\n{\n\t.product-item\n\t{\n\t\twidth: 100%;\n\t\theight: auto;\n\t}\n\t.product\n\t{\n\t\theight: auto;\n\t\tborder: none;\n\t}\n\t.product_name a\n\t{\n\t\tfont-size: 12px;\n\t}\n\t.add_to_cart_button\n\t{\n\t\tmargin-top: 20px;\n\t}\n}\n\n","id":"style-product-item"},{"style":{},"type":"div","class":"product_sorting_container product_sorting_container_top","children":[{"style":{},"type":"div","class":"pages d-flex flex-row justify-content-center","children":[{"style":{},"type":"nav","aria-label":"Page navigation example","children":[{"style":{},"type":"ul","class":"pagination ul-product-pagination","children":[{"style":{},"type":"li","class":"page-item","children":[{"style":{},"type":"a","class":"page-link","href":"#","html":"اولین","on":{},"version":"wb4","id":"a-firs