on":"wb4"},{"style":{},"type":"li","class":"page-item","children":[{"style":{},"type":"a","class":"page-link","href":"#","html":"1","on":{},"version":"wb4","id":"a-current"}],"on":{},"version":"wb4"},{"style":{},"type":"li","class":"page-item","children":[{"style":{},"type":"a","class":"page-link","href":"#","html":"بعدی","on":{},"version":"wb4","id":"a-next"}],"on":{},"version":"wb4"},{"style":{},"type":"li","class":"page-item","children":[{"style":{},"type":"a","class":"page-link","href":"#","html":"آخرین","on":{},"version":"wb4","id":"a-last"}],"on":{},"version":"wb4"}],"on":{},"version":"wb4","id":"ul-paginator"}],"on":{},"version":"wb4","id":"nav-paginator"}],"on":{},"version":"wb4","id":"div-pages"}],"on":{},"version":"wb4","id":"div-sorting-bottom"}],"on":{"init":"var catId = $routeParams.id;\nvar url = catId ? '/api/v2/shop/categories/' + catId + '/products'\n\t: '/api/v2/shop/products';\n\n$widget.options = {\n\tcurrency: $window.getMeta('theme:currency'),\n\tcurrency_code: $window.getMeta('theme:currency_code')\n};\n\nvar myScope = {\n\tpaginator: null,\n\tparams: null,\n\tcollectionBuilder: null\n};\n\nfunction init() {\n\t// Load Items\n\tmyScope = {\n\t\tcollectionBuilder: null,\n\t\tpaginator: null,\n\t\tparams: null\n\t};\n\tvar searchParams = getParams();\n\tloadItems(searchParams)\n\t\t.then(function() {\n\t\t\tinitPaginator(myScope.paginator);\n\t\t\tinitSorter(myScope.paginator);\n\t\t});\n}\n\ninit();\n\nfunction loadItems(params) {\n\treturn $http({\n\t\tmethod: 'GET',\n\t\turl: url,\n\t\tparams: params\n\t}).then(function(result) {\n\t\tvar items = result.data.items;\n\t\tvar dataList = [];\n\t\tvar pos = 1;\n\t\titems.forEach(function(item) {\n\t\t\t// Edit data of product\n\t\t\tvar data = createData(item);\n\t\t\tdata.position = pos++;\n\t\t\tdataList.push(data);\n\t\t});\n\t\tmyScope.paginator = result.data;\n\t\tcreateCollection(dataList);\n\t});\n}\n\nvar formattCurrency = function(val) {\n\treturn Number(val).toLocaleString();\n};\n\nvar toFarsiNumber = function(n) {\n\tconst farsiDigits = ['\\u06f0', '\\u06f1', '\\u06f2', '\\u06f3', '\\u06f4', '\\u06f5', '\\u06f6', '\\u06f7', '\\u06f8', '\\u06f9'];\n\treturn n\n\t\t.toString()\n\t\t.replace(/\\d/g, x => farsiDigits[x]);\n};\n\nfunction getParams() {\n\tif (myScope.params == null) { // Initiate params from URL\n\t\tvar params = $window.location.search;\n\t\tparams = new URLSearchParams(params);\n\t\tparams.set('graphql', '{items{id,title,price,off,avatar,categories{id,name}},counts,current_page,page_number,items_per_page}');\n\t\tif (!params.has('_px_sk')) {\n\t\t\tparams.set('_px_sk', 'price');\n\t\t\tparams.set('_px_so', 'd');\n\t\t}\n\t\tif (!params.has('_px_p')) {\n\t\t\tparams.set('_px_p', 1);\n\t\t}\n\t\tif (!params.has('_px_ps')) {\n\t\t\tparams.set('_px_ps', 32);\n\t\t}\n\t\tvar keys = params.keys();\n\t\tvar paramsJson = {};\n\t\tfor (const key of keys) {\n\t\t\tparamsJson[key] = params.get(key);\n\t\t}\n\t\tmyScope.params = paramsJson;\n\t}\n\treturn myScope.params;\n}\n\n// Edit data\nfunction createData(item) {\n\tvar data = angular.copy(item);\n\tdata.off = data.off ? data.off : 0;\n\tdata.price = data.price ? data.price : 0;\n\tdata.total_price = data.price - data.off;\n\t// Micro data\n\tdata.total_price_numerical = data.total_price;\n\tdata.currency_code = $widget.options.currency_code;\n\n\t// Format prices\n\tdata.price_txt = toFarsiNumber(formattCurrency(data.price)) + ' ' + $widget.options.currency;\n\tdata.total_price_txt = toFarsiNumber(formattCurrency(data.total_price)) + ' ' + $widget.options.currency;\n\tdata.off_txt = toFarsiNumber(formattCurrency(data.off));\n\n\tvar catList = [];\n\tdata.categories.forEach(function(cat) {\n\t\tcatList.push(cat.name);\n\t});\n\tdata.categoriesStr = catList.join(' ');\n\n\treturn data;\n}\n\nfunction createCollection(itemsList) {\n\t// Product-grid widget\n\tvar collectionWidget = $widget.getChildById('product-grid');\n\t// Clear collections\n\tcollectionWidget.removeChildren();\n\tif (myScope.collectionBuilder == null) { // First call, create and init collection builder\n\t\t// Get product template\n\t\tvar template = $widget.getChildById('div-templates')\n\t\t\t.getChildById('product-item-tmpl').getModel();\n\t\tvar template = angular.copy(template);\n\n\t\t// Create collection builder\n\t\tmyScope.collectionBuilder = new CollectionBuilder();\n\t\tmyScope.collectionBuilder.setTemplate(template)\n\t\t\t.setWidget(collectionWidget);\n\t}\n\tmyScope.collectionBuilder.setItems(itemsList)\n\t\t.build();\n}\n\nfunction initPaginator(pag) {\n\tvar ulPag = $('.ul-product-pagination');\n\tulPag.empty();\n\tvar params = $location.search();\n\tparams = new URLSearchParams(params);\n\tvar link = $location.absUrl();\n\tlink = link.indexOf('?') >= 0 ? link.substring(0, link.indexOf('?')) : link;\n\t{// Add pagination buttons\n\t\t// First button\n\t\tparams.set('_px_p', 1);\n\t\tvar href = link + '?' + params.toString();\n\t\tvar li = createPaginationBtn(1, href, '\\u0627\\u0648\\u0644\\u06cc\\u0646', 'pagination-btn', pag.current_page > 1);\n\t\tulPag.append(li);\n\t\t// Previous button\n\t\tvar i = Number(pag.current_page) - 1;\n\t\tparams.set('_px_p', i);\n\t\tvar href = link + '?' + params.toString();\n\t\tvar li = createPaginationBtn(i, href, '\\u0642\\u0628\\u0644\\u06cc', 'pagination-btn', pag.current_page > 1);\n\t\tulPag.append(li);\n\t\t// Current button\n\t\tvar li = createPaginationBtn(pag.current_page, '#', pag.current_page, 'pagination-btn', false);\n\t\tulPag.append(li);\n\t\t// Next button\n\t\tvar i = Number(pag.current_page) + 1;\n\t\tparams.set('_px_p', i);\n\t\tvar href = link + '?' + params.toString();\n\t\tvar li = createPaginationBtn(i, href, '\\u0628\\u0639\\u062f\\u06cc', 'pagination-btn', pag.current_page < pag.page_number);\n\t\tulPag.append(li);\n\t\t// Last button\n\t\tvar i = Number(pag.page_number);\n\t\tparams.set('_px_p', i);\n\t\tvar href = link + '?' + params.toString();\n\t\tvar li = createPaginationBtn(i, href, '\\u0622\\u062e\\u0631\\u06cc\\u0646', 'pagination-btn', pag.current_page < pag.page_number);\n\t\tulPag.append(li);\n\t}\n\n\t$('.pagination-btn').click(paginatorBtnClicked);\n}\n\nfunction initSorter(pag) {\n\tvar ulSorter = $('.ul-product-sorter');\n\tulSorter.empty();\n\tvar params = $location.search();\n\tparams = new URLSearchParams(params);\n\tvar link = $location.absUrl();\n\tlink = link.indexOf('?') >= 0 ? link.substring(0, link.indexOf('?')) : link;\n\t{// Add pagination buttons\n\t\t// Ascending price button\n\t\tparams.set('_px_sk', 'price');\n\t\tparams.set('_px_so', 'a');\n\t\tvar href = link + '?' + params.toString();\n\t\tvar li = createPaginationBtn('price#a', href, '\\u0627\\u0631\\u0632\\u0627\\u0646\\u200c\\u062a\\u0631\\u06cc\\u0646', 'sorter-btn', true);\n\t\tulSorter.append(li);\n\t\t// Descending price button\n\t\tparams.set('_px_sk', 'price');\n\t\tparams.set('_px_so', 'd');\n\t\tvar href = link + '?' + params.toString();\n\t\tvar li = createPaginationBtn('price#d', href, '\\u06af\\u0631\\u0627\\u0646\\u200c\\u062a\\u0631\\u06cc\\u0646', 'sorter-btn', true);\n\t\tulSorter.append(li);\n\t\t// Descending off button\n\t\tparams.set('_px_sk', 'off');\n\t\tparams.set('_px_so', 'd');\n\t\tvar href = link + '?' + params.toString();\n\t\tvar li = createPaginationBtn('off#d', href, '\\u0628\\u06cc\\u0634\\u062a\\u0631\\u06cc\\u0646 \\u062a\\u062e\\u0641\\u06cc\\u0641', 'sorter-btn', true);\n\t\tulSorter.append(li);\n\t\t// Newest button\n\t\tparams.set('_px_sk', 'id');\n\t\tparams.set('_px_so', 'd');\n\t\tvar href = link + '?' + params.toString();\n\t\tvar li = createPaginationBtn('id#d', href, '\\u062c\\u062f\\u06cc\\u062f\\u062a\\u0631\\u06cc\\u0646', 'sorter-btn', true);\n\t\tulSorter.append(li);\n\t}\n\n\t$('.sorter-btn').click(sorterBtnClicked);\n}\n\nfunction createPaginationBtn(index, href, text, innerClass, isEnabled) {\n\tvar li = $('<li class=\"page-item\"><a class=\"' + innerClass + ' page-link\" data-index=\"' + index + '\" href=\"' + href + '\">' + text + '</a></li>');\n\t//\tif (isActive) {\n\t//\t\tli.addClass('active');\n\t//\t}\n\tif (!isEnabled) {\n\t\tli.addClass('disabled');\n\t}\n\treturn li;\n}\n\nfunction paginatorBtnClicked(ev) {\n\tev.preventDefault();\n\tev.stopImmediatePropagation();\n\tev.stopPropagation();\n\n\t$('.page-item').removeClass('active');\n\tvar index = $(ev.target).attr('data-index');\n\tif ($widget.state == 'ready') {\n\t\tvar pag = myScope.paginator;\n\t\tif (pag.current_page != index) {\n\t\t\tvar params = getParams();\n\t\t\tparams._px_p = index;\n\t\t\tloadItems(params)\n\t\t\t\t.then(function() {\n\t\t\t\t\tinitPaginator(myScope.paginator);\n\t\t\t\t});\n\t\t}\n\t}\n}\n\nfunction sorterBtnClicked(ev) {\n\tev.preventDefault();\n\tev.stopImmediatePropagation();\n\tev.stopPropagation();\n\n\t$('.sorter-btn').parent().removeClass('active');\n\t$(ev.target).parent().addClass('active');\n\tvar index = $(ev.target).attr('data-index');\n\tif ($widget.state == 'ready') {\n\t\tvar params = getParams();\n\t\tvar sortOption = index.split('#');\n\t\tparams._px_sk = sortOption[0];\n\t\tparams._px_so = sortOption[1];\n\t\tloadItems(params);\n\t}\n}\n"},"version":"wb4","id":"div-products","itemscope":"true","itemtype":"http://schema.org/ItemList"},{"style":{},"type":"div","class":"sidebar","children":[{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":"\n/*********************************\n6. Sidebar\n*********************************/\n\n.sidebar\n{\n\tfloat: right;\n\twidth: 200px;\n\tdirection: rtl;\n}\n.sidebar_section\n{\n\tpadding-bottom: 35px;\n\tmargin-bottom: 50px;\n\tborder-bottom: solid 1px #ebebeb;\n}\n.sidebar_title\n{\n    text-align: center;\n}\n.sidebar_title h5\n{\n\tfont-size: 18px;\n\tmargin-bottom: 21px;\n}\n.sidebar_categories\n{\n\ttext-align: right;\n\tpadding: 0 18px;\n}\n.sidebar_categories li.active a\n{\n\tcolor: #fe4c50;\n}\n.sidebar_categories li.active:hover a\n{\n\tcolor: #fe4c50;\n}\n.sidebar_categories li a\n{\n\tfont-size: 14px;\n\tline-height: 40px;\n\tcolor: #51545f;\n}\n.sidebar_categories li a:hover\n{\n\tcolor: #b5aec4;\n}\n.sidebar_categories li a span\n{\n\tmargin-right: 5px;\n}\n#slider-range\n{\n\tmargin-left: 7px !important;\n}\n#amount\n{\n\tmargin-top: 8px;\n\tcolor: #1e1e27 !important;\n\tfont-size: 16px !important;\n\tfont-weight: 500 !important;\n}\n.filter_button\n{\n\twidth: 100%;\n\theight: 30px;\n\tbackground: #1e1e27;\n\ttext-align: center;\n\tmargin-top: 25px;\n\tcursor: pointer;\n\t-webkit-transition: all 0.3s ease;\n\t-moz-transition: all 0.3s ease;\n\t-ms-transition: all 0.3s ease;\n\t-o-transition: all 0.3s ease;\n\ttransition: all 0.3s ease;\n}\n.filter_button:hover\n{\n\tbackground: #34343f;\n}\n.filter_button span\n{\n\tfont-size: 12px;\n\tfont-weight: 600;\n\ttext-transform: uppercase;\n\tline-height: 30px;\n\tcolor: #FFFFFF;\n}\n.checkboxes li\n{\n\theight: 40px;\n\tline-height: 40px;\n}\n.checkboxes li.active i\n{\n\tcolor: #fe4c50;\n\tmargin-right: 21px;\n}\n.checkboxes li.active i::after\n{\n\tdisplay: block;\n\tposition: absolute;\n\ttop: 1px;\n\tleft: 1px;\n\tfont-family: 'FontAwesome';\n\tfont-size: 10px;\n\tcontent: '\\f00c';\n\tcolor: #FFFFFF;\n}\n.checkboxes\n{\n\theight: 200px;\n\toverflow: hidden;\n}\n.checkboxes.active\n{\n\theight: auto;\n}\n.checkboxes li i\n{\n\tposition: relative;\n\tcolor: #b3b7c8;\n\tcursor: pointer;\n\tmargin-right: 22px;\n}\n.checkboxes li span\n{\n\tfont-family: 'Poppins', sans-serif;\n\tcolor: #51545f;\n}\n.show_more\n{\n\tdisplay: inline-block;\n\tcursor: pointer;\n}\n.show_more span\n{\n\theight: 40px;\n\tline-height: 40px;\n\tcolor: #fe4c50;\n}\n.show_more span:first-child\n{\n\tmargin-right: 11px;\n}\n\n/*********************************\nSidebar Responsive\n*********************************/\n\n@media only screen and (max-width: 991px)\n{\n\t.sidebar\n\t{\n\t\tfloat: none;\n\t\twidth: 100%;\n\t\tmargin-top: 32px;\n\t}\n\t.filter_button\n\t{\n\t\twidth: 150px;\n\t}\n}","id":"style-sidebar"},{"style":{},"type":"div","class":"side