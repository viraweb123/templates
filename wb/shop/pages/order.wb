.count : 1;\n\tdata.total_price = data.price * data.count;\n\tdata.total_offed_price = data.offed_price * data.count;\n\treturn data;\n}\n\nfunction getPrice(item) {\n\treturn (item.price ? item.price : 0) * (item.count ? item.count : 1);\n}\n\nfunction getOffedPrice(item) {\n\treturn ((item.price ? item.price : 0) - (item.off ? item.off : 0))\n\t\t* (item.count ? item.count : 1);\n}\n\nfunction initOrderHeader(order) {\n\tvar title = $('#td-title');\n\tvar name = $('#td-name');\n\tvar email = $('#td-email');\n\tvar phone = $('#td-phone');\n\tvar address = $('#td-address');\n\n\ttitle.html(order.title);\n\tname.html(order.full_name);\n\temail.html(order.email);\n\tphone.html(order.phone);\n\taddress.html(order.address);\n}\n\nfunction initOrderItems(items) {\n\tvar totalPrice = 0;\n\tfor (var i = 0; i < items.length; i++) {\n\t\ttotalPrice += getOffedPrice(items[i]);\n\t\tvar item = createData(items[i]);\n\t\tvar itemStr = '<tr>';\n\t\titemStr += '<td>' + item.title + '</td>';\n\t\titemStr += '<td>' + item.count + '</td>';\n\t\titemStr += '<td>' + toFarsiCurrency(item.offed_price) + '</td>';\n\t\titemStr += '<td>' + toFarsiCurrency(item.total_offed_price) + '</td>';\n\t\titemStr += '</tr>';\n\t\t$('#table-body').append(itemStr);\n\t}\n\t// Price row\n\tvar itemStr = '<tr>';\n\titemStr += '<td>\\u0645\\u062c\\u0645\\u0648\\u0639</td>';\n\titemStr += '<td colspan=\"2\"></td>';\n\titemStr += '<td class=\"accent-text\">' + formatPrice(totalPrice) + '</td>';\n\titemStr += '</tr>';\n\t$('#table-body').append(itemStr);\n}\n\nfunction initPayment(order) {\n\tvar payDiv = $widget.getChildById('div-payment');\n\tvar msgP = $widget.getChildById('div-messages')\n\t\t.getChildById('message');\n\tif (order.payment && order.payment.payRef) {\n\t\tvar str = $widget.options.payedMsg;\n\t\tstr = str.replace('{payRef}', order.payment.payRef);\n\t\tif (order.payment.backend) {\n\t\t\tstr = str.replace('{backend}', order.payment.backend.title);\n\t\t}\n\t\tpayDiv.setProperty('style.display', 'none');\n\t\tmsgP.setProperty('html', str);\n\t\tmsgP.setProperty('style.color', $widget.options.payedMsgColor);\n\t}\n}\n\n"},"version":"wb4","id":"order-info","children":[{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":".order-header {\n    padding: 0px;\n    margin: 20px 0px;\n    border: 1px dashed rgb(196, 198, 207);\n    border-radius: 4px;\n}\n\n.accent-text {\n    font-size: larger;\n    font-weight: bold;\n    color: #fe4c50;\n}\n\n.table-rtl {\n    direction: rtl;\n    text-align: right;\n}","id":"style-order-info"},{"style":{},"type":"div","name":"div","on":{},"version":"wb4","class":"table-responsive-md order-header","id":"table-general-info","children":[{"style":{},"type":"table","name":"Table","on":{},"version":"wb4","class":"table table-hover table-borderless table-sm table-rtl","children":[{"style":{},"type":"tbody","name":"Table Body","on":{},"version":"wb4","children":[{"style":{},"type":"tr","children":[{"style":{},"type":"th","scope":"row","children":[],"on":{},"version":"wb4","html":"نام و نام خانوادگی:"},{"style":{},"type":"td","children":[],"on":{},"version":"wb4","id":"td-name"}],"on":{},"version":"wb4"},{"style":{},"type":"tr","children":[{"style":{},"type":"th","scope":"row","children":[],"on":{},"version":"wb4","html":"شماره تماس:"},{"style":{},"type":"td","children":[],"on":{},"version":"wb4","id":"td-phone"}],"on":{},"version":"wb4"},{"style":{},"type":"tr","children":[{"style":{},"type":"th","scope":"row","children":[],"on":{},"version":"wb4","html":"ایمیل:"},{"style":{},"type":"td","children":[],"on":{},"version":"wb4","id":"td-email"}],"on":{},"version":"wb4"},{"style":{},"type":"tr","children":[{"style":{},"type":"th","scope":"row","children":[],"on":{},"version":"wb4","html":"آدرس:"},{"style":{},"type":"td","children":[],"on":{},"version":"wb4","id":"td-address"}],"on":{},"version":"wb4"}]}]}]},{"style":{},"type":"div","name":"div","on":{},"version":"wb4","class":"table-responsive-md","id":"table-item