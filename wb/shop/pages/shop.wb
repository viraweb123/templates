graphql: '{items{id,title,price,off,avatar,categories{id,name}}}',\n\t\t\t_px_sk: 'id',\n\t\t\t_px_so: 'd'\n\t\t}\n\t}).then(function(result) {\n\t\tvar items = result.data.items;\n\t\tvar productList = [];\n\t\tvar allCats = [{ id: 0, name: 'همه', default: true }];\n\t\titems.forEach(function(item) {\n\t\t\t// Collect all categories[{\"style\":{},\"type\":\"meta\",\"content\":\"{{currency_code}}\",\"children\":[],\"on\":{},\"version\":\"wb4\",\"id\":\"meta-currency\",\"itemprop\":\"priceCurrency\"},{\"style\":{},\"type\":\"meta\",\"children\":[],\"on\":{},\"version\":\"wb4\",\"id\":\"meta-url\",\"itemprop\":\"url\",\"content\":\"/wb/shop/product?id={{id}}\"},{\"style\":{},\"type\":\"meta\",\"content\":\"http://schema.org/OnlineOnly\",\"children\":[],\"on\":{},\"version\":\"wb4\",\"id\":\"meta-availablility\",\"itemprop\":\"availability\"}]\n\t\t\titem.categories.forEach(function(cat) {\n\t\t\t\t// add if not exist\n\t\t\t\tif (allCats.map(function(e) { return e.id; }).indexOf(cat.id) == -1) {\n\t\t\t\t\tallCats.push(cat);\n\t\t\t\t}\n\t\t\t});\n\t\t\t// Edit data of product\n\t\t\tvar data = createData(item);\n\t\t\tproductList.push(data);\n\t\t});\n\t\t//    createFiltersCollection(allCats);\n\t\tcreateProductsCollection(productList);\n\t});\n}\n\ninit();\n\n\nvar formattCurrency = function(val) {\n\treturn Number(val).toLocaleString();\n};\n\nvar toFarsiNumber = function(n) {\n\tconst farsiDigits = ['۰', '۱', '۲', '۳', '۴', '۵', '۶', '۷', '۸', '۹'];\n\treturn n\n\t\t.toString()\n\t\t.replace(/\\d/g, x => farsiDigits[x]);\n};\n\n// Edit data\nfunction createData(item) {\n\tvar data = angular.copy(item);\n\tdata.off = data.off ? data.off : 0;\n\tdata.price = data.price ? data.price : 0;\n\tdata.total_price = data.price - data.off;\n\t// Microdata\n\tdata.price_numerical = data.total_price;\n\tdata.currency_code = $widget.options.currency_code;\n\t// Format prices\n\tdata.price_txt = toFarsiNumber(formattCurrency(data.price)) + ' ' + $widget.options.currency;\n\tdat