, seconds;\n\n\tvar d = $widget.getChildById('li-day').getChildById('day');\n\tvar h = $widget.getChildById('li-hour').getChildById('hour');\n\tvar m = $widget.getChildById('li-minute').getChildById('minute');\n\tvar s = $widget.getChildById('li-second').getChildById('second');\n\n\tsetInterval(function() {\n\t\t// find the amount of \"seconds\" between now and target\n\t\tvar current_date = new Date().getTime();\n\t\tvar seconds_left = (target_date - current_date) / 1000;\n\n\t\t// do some time calculations\n\t\tdays = parseInt(seconds_left / 86400);\n\t\tseconds_left = seconds_left % 86400;\n\n\t\thours = parseInt(seconds_left / 3600);\n\t\tseconds_left = seconds_left % 3600;\n\n\t\tminutes = parseInt(seconds_left / 60);\n\t\tseconds = parseInt(seconds_left % 60);\n\n\t\t// display result\n\t\td.setProperty('html', days);\n\t\th.setProperty('html', hours);\n\t\tm.setProperty('html', minutes);\n\t\ts.setProperty('html', seconds);\n\n\t}, 1000);\n\n}\n\ninitTimer();"},"version":"wb4","id":"ul-timer"},{"style":{},"type":"div","class":"red_button deal_ofthe_week_button","children":[{"style":{},"type":"a","href":"#","on":{},"version":"wb4","html":"مشاهده"}],"on":{},"version":"wb4"}],"on":{},"version":"wb4","id":"div-timer"}],"on":{},"version":"wb4"}],"on":{},"version":"wb4"}],"on":{},"version":"wb4"}],"on":{},"version":"wb4","id":"deal_of_the_week"}],"url":"/api/v2/cms/contents/shop-common-widgets/content#deal_of_the_week","id":"import-deal_of_the_week"},{"style":{},"type":"div","class":"best_sellers","children":[{"style":{},"type":"style","name":"style","on":{},"version":"wb4","text":"\n\n/*********************************\n7. Best Sellers\n*********************************/\n\n.product_slider_container\n{\n\twidth: 100%;\n\theight: 340px;\n\tmargin-top: 73px;\n}\n.product_slider_container::after\n{\n\tdisplay: block;\n\tposition: absolute;\n\ttop: 0;\n\tright: 0;\n\twidth: 1px;\n\theight: 100%;\n\tbackground: #FFFFFF;\n\tcontent: '';\n\tz-index: 1;\n}\n.product_slide