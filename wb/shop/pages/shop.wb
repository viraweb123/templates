ms: {\n\t\t\tgraphql: '{items{id,name,title,description,creation_dtime,modif_dtime,author{login},metas{key,value},term_taxonomies{taxonomy,count,term{name}}}}',\n\t\t\t_px_fk: 'media_type',\n\t\t\t_px_fv: 'page',\n\t\t\t_px_sk: 'id',\n\t\t\t_px_so: 'd',\n\t\t\t_px_ps: 6\n\t\t}\n\t}).then(function(result) {\n\t\tvar items = result.data.items;\n\t\tvar dataList = [];\n\t\tvar pos = 1;\n\t\titems.forEach(function(item) {\n\t\t\t// Edit data of product\n\t\t\tvar data = editData(item);\n\t\t\tdata.position = pos++;\n\t\t\tdataList.push(data);\n\t\t});\n\t\tcreatePostsCollection(dataList);\n\t});\n\n}\n\ninit();\n\n//*********************************** Helper Function\n\n// Edit data\nfunction editData(item) {\n\t// Add metas\n\titem.metamap = {};\n\titem.metas.forEach(function(meta) {\n\t\tvar k = meta.key.replace(/\\./g, '_');\n\t\titem.metamap[k] = meta.value;\n\t});\n\n\t// Add tags\n\titem.tags = [];\n\titem.term_taxonomies.forEach(function(tt) {\n\t\tif (tt.taxonomy == 'tag') {\n\t\t\tvar val = {\n\t\t\t\tname: tt.term.name,\n\t\t\t\tcount: tt.count\n\t\t\t};\n\t\t\titem.tags.push(val);\n\t\t}\n\t});\n\t// Data for microdata\n\titem.creation_dtime_iso = new Date(item.creation_dtime).toISOString();\n\titem.modif_dtime_iso = new Date(item.modif_dtime).toISOString();\n\treturn item;\n}\n\n\nfunction createPostsCollection(postList) {\n\t// Get post item template\n\tvar template = $widget.getChildById('div-templates')\n\t\t.getChildById('blog-item-tmpl').getModel();\n\tvar template = angular.copy(template);\n\n\t// Build collection\n\tvar collectionBuilder = new CollectionBuilder();\n\tcollectionBuilder.setTemplate(template)\n\t\t.setItems(postList)\n\t\t.setWidget(postsWidget)\n\t\t.build();\n}\n\n\n"},"version":"wb4","id":"blog-container"}],"on":{},"version":"wb4","id":"blogs"},{"style":{},"type":"import","name":"Import","on":{},"version":"wb4","children":[{"style":{},"type":"div","class":"newsletter","children":[{"style":{},"type":"style","name":"style","on":{},"vers