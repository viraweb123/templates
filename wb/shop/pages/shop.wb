e - data.off;\n\t// Format prices\n\tdata.price = toFarsiNumber(formattCurrency(data.price)) + ' ' + $widget.options.currency;\n\tdata.total_price = toFarsiNumber(formattCurrency(data.total_price)) + ' ' + $widget.options.currency;\n\tdata.off_txt = toFarsiNumber(formattCurrency(data.off));\n\n\tvar catList = [];\n\tdata.categories.forEach(function(cat) {\n\t\tcatList.push(cat.name);\n\t});\n\tdata.categoriesStr = catList.join(' ');\n\n\treturn data;\n}\n\nfunction createProductsCollection(productList) {\n\t// Get product template\n\tvar template = $widget.getChildById('div-templates')\n\t\t.getChildById('template')\n\t\t.getChildById('slider-item-tmpl').getModel();\n\tvar template = angular.copy(template);\n\n\t// Build collection\n\tvar collectionBuilder = new CollectionBuilder();\n\tcollectionBuilder.setTemplate(template)\n\t\t.setItems(productList)\n\t\t.setWidget(productsWidget)\n\t\t.build()\n\t\t.then(initSlider);\n}\n\n/* \n * 8. Init Slider\n */\n\nfunction initSlider() {\n\tif ($('.product_slider').length) {\n\t\tvar slider1 = $('.product_slider');\n\n\t\tslider1.owlCarousel({\n\t\t\tloop: false,\n\t\t\tdots: false,\n\t\t\tnav: false,\n\t\t\tresponsive:\n\t\t\t{\n\t\t\t\t0: { items: 1 },\n\t\t\t\t480: { items: 2 },\n\t\t\t\t768: { items: 3 },\n\t\t\t\t991: { items: 4 },\n\t\t\t\t1280: { items: 5 },\n\t\t\t\t1440: { items: 5 }\n\t\t\t}\n\t\t});\n\n\t\tif ($('.product_slider_nav_left').length) {\n\t\t\t$('.product_slider_nav_left').on('click', function() {\n\t\t\t\tslider1.trigger('prev.owl.carousel');\n\t\t\t});\n\t\t}\n\n\t\tif ($('.product_slider_nav_right').length) {\n\t\t\t$('.product_slider_nav_right').on('click', function() {\n\t\t\t\tslider1.trigger('next.owl.carousel');\n\t\t\t});\n\t\t}\n\t}\n}\n\n\n"},"version":"wb4","id":"slider-container"}],"on":{},"version":"wb4","id":"best_sellers"},{"style":{},"type":"import","name":"Import","on":{},"version":"wb4","children":[{"style":{},"type":"div","class":"benefit","children":[{"style":{},"type":"style","